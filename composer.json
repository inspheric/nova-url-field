{
    "name": "inspheric/nova-url-field",
    "description": "A Laravel Nova URL field.",
    "keywords": [
        "laravel",
        "nova",
        "field",
        "url"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "laravel/nova": "^4.0|^5.0"
    },
    "autoload": {
        "psr-4": {
            "Inspheric\\Fields\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Inspheric\\Fields\\UrlFieldServiceProvider"
            ]
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
